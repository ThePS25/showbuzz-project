{
	"compilerOptions": {
		"target": "es5",
		"module": "esnext",
		"moduleResolution": "node",
		"baseUrl": "./",
		"paths": {
			"*": ["./src/*"]
		},
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"skipLibCheck": true,
		"jsx": "react-jsx"
	},
	"include": ["src", "server.js"],
	"exclude": ["node_modules", "dist"]
}
